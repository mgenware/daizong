{
  "name": "daizong",
  "version": "0.0.0",
  "description": "Command runner. Better package.json scripts.",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/daizong",
  "bugs": "https://github.com/mgenware/daizong/issues",
  "keywords": [
    "process",
    "task runner"
  ],
  "bin": {
    "daizong": "./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/daizong.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "mocha --require source-map-support/register dist_tests/**/*.test.js",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --max-warnings 0 --ext .ts src/ tests/",
    "clean": "del-cli dist && del-cli dist_tests"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.css",
    "/dist/**/*.map"
  ],
  "dependencies": {
    "chalk": "^4.0.0",
    "cosmiconfig": "^6.0.0",
    "meow": "^7.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.2",
    "source-map-support": "^0.5.19",
    "typescript": "^3.9.3"
  }
}
