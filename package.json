{
  "name": "daizong",
  "version": "0.7.0",
  "description": "package.json scripts runner",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/daizong",
  "bugs": "https://github.com/mgenware/daizong/issues",
  "keywords": [
    "process",
    "task runner"
  ],
  "bin": {
    "daizong": "./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/daizong.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "mocha --require source-map-support/register dist_tests/**/*.test.js",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --max-warnings 0 --ext .ts src/ tests/",
    "clean": "del-cli dist dist_tests"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.css",
    "/dist/**/*.map"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "cosmiconfig": "^6.0.0",
    "del": "^5.1.0",
    "make-dir": "^3.1.0",
    "meow": "^7.0.1",
    "p-map": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript-lite": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.0.1",
    "source-map-support": "^0.5.19",
    "typescript": "^3.9.7"
  }
}
